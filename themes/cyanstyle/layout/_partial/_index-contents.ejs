<script type="text/javascript">

var slideIndex = 1;
var slideImages, slideImgCnt;

function plusDivs(n) {
  showDivs(slideIndex += n);
}

function showDivs(n) {
  var i;
  
  if (n > slideImgCnt) {
    slideIndex = 1;
  }
  if (n < 1) {
    slideIndex = slideImgCnt;
  }
  for (i = 0; i < slideImgCnt; i++) {
    if (slideImages[i].style.display != "none")
      slideImages[i].style.display = "none";
  }
  slideImages[slideIndex-1].style.display = "block";
}

function initializeGallery() {
  slideImages = document.getElementsByClassName("slideshow");
  slideImgCnt = slideImages.length;

  showDivs(slideIndex);
}

window.onload = initializeGallery;
</script>

